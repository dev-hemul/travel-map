{
  "name": "travel-map-root",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint \"client/src/**/*.{js,jsx}\" \"server/**/*.{js,jsx}\"",
    "format": "prettier --write \"client/**/*.{js,jsx,json,css,md}\" \"server/**/*.{js,jsx,json,css,md}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint . --fix --no-cache --max-warnings=0"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.5.3"
  },
  "dependencies": {}
}
